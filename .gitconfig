[remote "Tomas-M"]
    url = git@github.com:Tomas-M/linux-live.git
    fetch = +refs/heads/*:refs/remotes/Tomas-M/*
[remote "origin"]
    url = git@github.com:huangyingw/Tomas-M_linux-live.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = Tomas-M
    target = Tomas-M/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
    ignore = run.sh
[deploy]
	host = localhost
	path = ~/myproject/git/tools/Tomas-M/linux-live
